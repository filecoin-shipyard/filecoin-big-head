{
  "name": "filecoin-big-head",
  "version": "0.0.11",
  "description": "Displays Filecoin chain height in big letters in terminal",
  "scripts": {
    "dev": "nodemon --ignore dist --exec babel-node index.js",
    "build": "rm -rf dist; mkdir dist; babel *.js --out-dir dist",
    "prepublish": "npm run build"
  },
  "bin": {
    "filecoin-big-head": "dist/index.js"
  },
  "keywords": [],
  "author": "Jim Pick (@jimpick)",
  "license": "MIT",
  "dependencies": {
    "@jimpick/ink-watch-for-exit-key": "^0.0.6",
    "@jimpick/use-filecoin-config": "^0.0.7",
    "@jimpick/use-filecoin-head": "^0.0.7",
    "@jimpick/use-filecoin-network-info": "^0.0.6",
    "filecoin-api-client": "^0.3.0",
    "ink": "^2.3.0",
    "ink-big-text": "^1.0.1",
    "meow": "^5.0.0",
    "node-fetch": "^2.3.0",
    "react": "^16.8.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/preset-env": "^7.5.5",
    "babel-plugin-shebang": "^1.0.0",
    "nodemon": "^1.19.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "shebang",
      "@babel/transform-react-jsx"
    ]
  },
  "browserslist": "node 10",
  "nodemonConfig": {
    "stdin": false,
    "restartable": false
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jimpick/filecoin-big-head.git"
  }
}
